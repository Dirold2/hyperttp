{
  "name": "hyperttp",
  "version": "0.1.5",
  "description": "A high-performance, universal HTTP client for Node.js with caching, retries, queueing, rate limiting, cookies and logging.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "dirold2",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dirold2/hyperttp.git"
  },
  "keywords": [
    "http",
    "client",
    "fetch",
    "undici",
    "cache",
    "rate-limit",
    "queue",
    "retry",
    "cookies",
    "logger"
  ],
  "dependencies": {
    "undici": "^7.16.0",
    "tough-cookie": "^6.0.0",
    "http-cookie-agent": "^7.0.3",
    "fast-xml-parser": "^5.3.3",
    "lru-cache": "^11.2.4"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.16",
    "eslint": "^9.39.2",
    "prettier": "3.7.4",
    "chalk": "5.6.2",
    "tsx": "^4.21.0",
    "dotenv-cli": "^11.0.0"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint 'src/**/*.{ts,js}'",
    "format": "prettier --write .",
    "test": "vitest run",
    "dev": "tsx src/index.ts"
  }
}
